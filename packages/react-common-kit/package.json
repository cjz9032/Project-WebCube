{
  "name": "react-common-kit",
  "version": "1.1.1",
  "description": "Wrappers for some common used react components and utilities",
  "author": "dexteryy <dexter.yy@gmail.com>",
  "contributors": [],
  "bugs": {
    "url": "https://github.com/dexteryy/react-common-kit/issues"
  },
  "homepage": "https://github.com/dexteryy/react-common-kit",
  "repository": "https://github.com/dexteryy/react-common-kit",
  "license": "MIT",
  "webcube:module": "src/index.js",
  "module": "es6/index.js",
  "main": "dist/index.js",
  "sideEffects": false,
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clear && npm run build:dist && npm run build:es6",
    "build:dist": "BABEL_ENV=dist babel src --out-dir dist",
    "build:es6": "babel src --out-dir es6",
    "clear": "rm -r dist es6 2>/dev/null || true"
  },
  "peerDependencies": {
    "react": ">= 16.3"
  },
  "dependencies": {
    "@babel/preset-es2016": "^7.0.0-beta.53",
    "hoist-non-react-statics": "^3.0.1",
    "lodash-decorators": "^6.0.0",
    "react-display-name": "^0.2.4",
    "react-onclickoutside": "^6.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
    "@babel/plugin-proposal-decorators": "^7.0.0-rc.1",
    "@babel/plugin-proposal-function-bind": "^7.0.0-rc.1",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0-rc.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-rc.1",
    "@babel/plugin-proposal-optional-catch-binding": "^7.0.0-rc.1",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-rc.1",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0-rc.1",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-rc.1",
    "@babel/polyfill": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "@babel/preset-typescript": "^7.0.0-rc.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-graphql-tag": "^1.6.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-ramda": "^1.6.2",
    "babel-plugin-styled-components": "^1.5.1",
    "fast-async": "^7.0.0"
  },
  "keywords": [
    "react",
    "component",
    "modal",
    "dialog",
    "confirm"
  ]
}
