{
  "name": "redux-cube",
  "version": "1.0.2",
  "description": "App state manager. A set of wrappers which simplify the use of Redux and its mainstream ecosystem, reduce boilerplate, and support many patterns (like Sub App, Reducer Bundle, ...)",
  "author": "dexteryy <dexter.yy@gmail.com>",
  "contributors": [],
  "bugs": {
    "url": "https://github.com/dexteryy/Project-WebCube/issues"
  },
  "homepage": "https://github.com/dexteryy/Project-WebCube/tree/master/packages/redux-cube",
  "repository": "https://github.com/dexteryy/Project-WebCube/tree/master/packages/redux-cube",
  "license": "MIT",
  "engines": {
    "node": ">=6.9.0"
  },
  "webcube:module": "src/index.js",
  "module": "src/index.js",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clear:babel && npm run build:babel",
    "build:babel": "babel src --out-dir dist --source-maps",
    "clear:babel": "rm -r dist 2>/dev/null || true"
  },
  "peerDependencies": {
    "react": ">= 15.x",
    "rxjs": ">= 5.5.5"
  },
  "dependencies": {
    "change-case": "^3.0.1",
    "lodash": "^4.17.4",
    "react-common-kit": "^1.1.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-debounced": "^0.4.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.18.0",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "topologically-combine-reducers": "^0.0.5"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0-beta.44",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-ramda": "^1.5.0",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-optional-chaining": "^7.0.0-alpha.13",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "fast-async": "6.3.1"
  },
  "keywords": [
    "redux",
    "redux-cube",
    "boilerplate",
    "webcube"
  ]
}
