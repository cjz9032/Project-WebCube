
WEBCUBE_DEVSERVER_HOST=localhost
WEBCUBE_DEVSERVER_PORT=8010

# https://webpack.github.io/docs/webpack-dev-server.html#webpack-dev-server-cli
# https://webpack.github.io/docs/webpack-dev-middleware.html#options
WEBCUBE_DEVSERVER_NOINFO=1
WEBCUBE_DEVSERVER_STATS_COLOR=1
WEBCUBE_DEVSERVER_HISTORYAPI=
WEBCUBE_DEVSERVER_WATCH_TIMEOUT=
WEBCUBE_DEVSERVER_WATCH_POLL=
# https://github.com/FormidableLabs/webpack-dashboard#options
WEBCUBE_DEVSERVER_DASHBOARD_PORT=

# https://github.com/dexteryy/Project-WebCube/tree/master/examples/webcube-initial-structure/#structure
WEBCUBE_CUSTOM_CONFIG_ROOT=
WEBCUBE_CUSTOM_STATIC_ROOT=
WEBCUBE_CUSTOM_STATIC_ROOT_URL=
WEBCUBE_OUTPUT_CUSTOM_ROOT=
WEBCUBE_OUTPUT_CUSTOM_HTML_ROOT=
WEBCUBE_OUTPUT_CUSTOM_JS_PATH=
WEBCUBE_OUTPUT_CUSTOM_CSS_PATH=
WEBCUBE_OUTPUT_CUSTOM_ASSETS_PATH=
WEBCUBE_OUTPUT_MANIFEST_NAME=
WEBCUBE_OUTPUT_MANIFEST_PREFIX=

# https://github.com/yaniswang/HTMLHint
WEBCUBE_DISABLE_HTMLHINT=

# https://github.com/kangax/html-minifier
WEBCUBE_DISABLE_HTMLMIN=

# https://github.com/facebook/react/issues/436
# https://github.com/zilverline/react-tap-event-plugin
WEBCUBE_DISABLE_TAP_EVENT_ADDON=

# https://github.com/webpack/docs/wiki/optimization#deduplication
# https://github.com/webpack/webpack/issues/959
WEBCUBE_ENABLE_DEDUPE_PLUGIN=

# http://babeljs.io/docs/plugins/transform-es2015-for-of/#optionsloose
# http://2ality.com/2015/12/babel6-loose-mode.html
# https://github.com/babel/babel/tree/master/experimental/babel-preset-env#target-chrome-52-with-webpack-2rollup-and-loose-mode
WEBCUBE_ENABLE_LOOSE_MODE=

# https://preactjs.com/guide/differences-to-react
# https://preactjs.com/guide/switching-to-preact
WEBCUBE_USE_PREACT=

# https://github.com/babel/babel/tree/master/experimental/babel-preset-env#forcealltransforms
# https://github.com/mishoo/UglifyJS2/tree/harmony
# https://www.npmjs.com/package/gulp-uglify-es
WEBCUBE_USE_UGLIFY=

WEBCUBE_OUTPUT_DISABLE_CACHE=
WEBCUBE_OUTPUT_DISABLE_MINIMIZE=
WEBCUBE_ENABLE_PROD_SOURCEMAP=

# https://www.npmjs.com/package/url-loader
WEBCUBE_DATAURL_IMAGES_LIMIT=25000
WEBCUBE_DATAURL_FONT_LIMIT=25000

# https://www.npmjs.com/package/image-webpack-loader
# To optimize the speed of installing dependencies,
# image-webpack-loader is not included in webcube's package.json.
# Please add it to app's package.json if you want to enable this option
WEBCUBE_ENABLE_IMAGE_MIN=

# https://github.com/babel/minify/tree/master/packages/babel-plugin-minify-mangle-names
WEBCUBE_MINIFY_KEEP_NAME=

# http://christianalfoni.github.io/react-webpack-cookbook/Split-app-and-vendors.html
# https://webpack.github.io/docs/list-of-plugins.html#commonschunkplugin
WEBCUBE_ENABLE_COMMON_CHUNK=1
# https://www.npmjs.com/package/extract-text-webpack-plugin
WEBCUBE_ENABLE_EXTRACT_CSS=1

# http://christianalfoni.github.io/react-webpack-cookbook/Split-app-and-vendors.html
WEBCUBE_COMMON_CORE_MODULES=["react", "react-dom", "react-helmet", "hoist-non-react-statics"]
WEBCUBE_COMMON_PROJECT_MODULES=[]

# https://webpack.github.io/docs/configuration.html#module-loaders
# An array of paths (relative to root directory) where the imported files will be transformed by the babel-loader
WEBCUBE_ES6_MODULES=["node_modules/redux-cube-with-router", "node_modules/redux-cube-with-persist", "node_modules/redux-cube-with-immutable", "node_modules/redux-cube", "node_modules/redux-source", "node_modules/redux-source-immutable", "node_modules/webcube", "node_modules/hifetch"]

# https://github.com/localForage/localForage/issues/577
WEBCUBE_WEBPACK_NO_PARSE=["node_modules/localforage/dist"]

# https://github.com/postcss/autoprefixer
# https://github.com/ai/browserslist#queries
WEBCUBE_BROWSERS=["Android 2.3", "Android >= 4", "Chrome >= 16", "Firefox >= 31", "Explorer >= 9", "iOS >= 7", "Opera >= 12", "Safari >= 7.1"]

WEBCUBE_DISABLE_CSSNEXT=

# https://github.com/webpack-contrib/css-loader#modules
WEBCUBE_DISABLE_CSS_MODULES=

# https://github.com/ant-design/ant-design/issues/3442
# paths relative to root directory
WEBCUBE_EXCLUDE_FROM_CSS_MODULES=["node_modules"]

# https://github.com/kriasoft/isomorphic-style-loader
# Same as WEBCUBE_EXCLUDE_FROM_CSS_MODULES
WEBCUBE_INCLUDE_INTO_ISORMOPHIC_STYLES=[]

# https://webpack.github.io/docs/list-of-plugins.html#defineplugin
WEBCUBE_CLIENT_ENV_VARS=["WEBCUBE_DISABLE_TAP_EVENT_ADDON", "WEBCUBE_USE_PREACT", "DEPLOY_ENV", "DEPLOY_MODE"]

WEBCUBE_STATIC_SERVER_ERROR_PAGE=
WEBCUBE_STATIC_SERVER_ENABLE_GZIP=
WEBCUBE_STATIC_SERVER_ENABLE_DEBUG=

# https://github.com/dexteryy/Project-WebCube/tree/master/packages/webcube/#how-to-deploy-the-web-app
# https://github.com/dexteryy/Project-WebCube/blob/master/packages/webcube/utils/index.js
WEBCUBE_DEPLOY_STATIC_CLOUD=oss
WEBCUBE_DEPLOY_STATIC_ROOT=http://my-domain-to-oss-bucket.com/static/
WEBCUBE_DEPLOY_STATIC_HTML_ROOT=http://my-domain-to-oss-bucket.com
WEBCUBE_DEPLOY_STAGING_STATIC_ROOT=http://staging.my-domain-to-oss-bucket.com/static/
WEBCUBE_DEPLOY_STAGING_STATIC_HTML_ROOT=http://staging.my-domain-to-oss-bucket.com
#WEBCUBE_DEPLOY_STATIC_CLOUD=s3
#WEBCUBE_DEPLOY_STATIC_ROOT=http://my-domain-to-s3-bucket.com/static/
#WEBCUBE_DEPLOY_STATIC_HTML_ROOT=http://my-domain-to-s3-bucket.com
#WEBCUBE_DEPLOY_STAGING_STATIC_ROOT=http://staging.my-domain-to-s3-bucket.com/static/
#WEBCUBE_DEPLOY_STAGING_STATIC_HTML_ROOT=http://staging.my-domain-to-s3-bucket.com

# https://github.com/dexteryy/Project-WebCube/blob/master/packages/webcube/utils/staticcloud/oss.js
WEBCUBE_DEPLOY_OSS_ENDPOINT=http://oss-cn-hangzhou.aliyuncs.com
WEBCUBE_DEPLOY_OSS_ID=
WEBCUBE_DEPLOY_OSS_SECRET=
WEBCUBE_DEPLOY_OSS_BUCKET=mybucket
WEBCUBE_DEPLOY_STAGING_OSS_BUCKET=mybucket-staging

# https://github.com/dexteryy/Project-WebCube/blob/master/packages/webcube/utils/staticcloud/s3.js
WEBCUBE_DEPLOY_S3_ENDPOINT=http://s3.amazonaws.com
WEBCUBE_DEPLOY_S3_ID=
WEBCUBE_DEPLOY_S3_SECRET=
WEBCUBE_DEPLOY_S3_BUCKET=mybucket
WEBCUBE_DEPLOY_STAGING_S3_BUCKET=mybucket-staging

# Multiple entry points
