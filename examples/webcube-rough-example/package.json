{
  "private": true,
  "name": "webcube-rough-example",
  "version": "2.0.1",
  "scripts": {
    "precommit:custom": "create-cube test",
    "test": "create-cube test",
    "dev": "DEPLOY_MODE=static create-cube dev",
    "build": "node --max-old-space-size=4096 node_modules/.bin/create-cube build -a",
    "clean": "create-cube build --only-clean",
    "serve": "create-cube serve",
    "deploy": "create-cube deploy"
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions"
  ],
  "webcube": {
    "css": {
      "cssModules": {
        "enable": true
      }
    },
    "output": {
      "disableAllChunksPreload": true,
      "disableInitialPreload": true
    },
    "dev": {
      "port": 8013
    },
    "deploy": {
      "ssrServer": {
        "warmUpUrls": [
          "/counter",
          "/number"
        ]
      }
    }
  },
  "dependencies": {
    "immutability-helper": "^2.7.1",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "react": "^16.4.2",
    "react-common-kit": "^1.1.3",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.0.5",
    "react-loadable": "^5.5.0",
    "react-loaders": "^3.0.1",
    "react-router-dom": "^4.3.1",
    "react-with-scripts": "^1.0.5",
    "redux-cube": "^1.1.4",
    "redux-cube-with-router": "^1.1.1",
    "redux-source": "^1.0.7",
    "redux-source-connect": "^1.0.6",
    "redux-source-with-block-ui": "^1.0.9",
    "redux-source-with-notify": "^1.1.6",
    "styled-components": "^3.4.2",
    "styled-sanitize": "^1.1.1",
    "webcube": "^2.0.0-beta.8"
  }
}
