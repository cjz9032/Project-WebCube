{
  "private": true,
  "scripts": {
    "precommit": "create-cube precommit",
    "update": "create-cube update && echo 7.0 > .project-version",
    "upgrade": "create-cube update -c && echo 7.0 > .project-version",
    "lint": "create-cube lint",
    "lint:error": "create-cube lint --quiet"
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,mjsx,es6,css,scss,less}": [
      "npm run lint:error --",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "immutability-helper": "^2.7.1",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "polished": "^1.9.3",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "^16.4.2",
    "react-common-kit": "^1.1.1",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.0.5",
    "react-loadable": "^5.5.0",
    "react-loaders": "^3.0.1",
    "react-router-dom": "^4.3.1",
    "react-with-scripts": "^1.0.3",
    "redux-cube": "^1.0.2",
    "redux-cube-with-router": "^1.0.10",
    "redux-source": "^1.0.5",
    "redux-source-connect": "^1.0.3",
    "redux-source-with-block-ui": "^1.0.6",
    "redux-source-with-notify": "^1.1.4",
    "rxjs": "^6.2.2",
    "styled-components": "^3.4.2",
    "styled-icons": "^2.3.2",
    "styled-normalize": "^8.0.0",
    "styled-sanitize": "^1.1.1",
    "webcube": "^2.0.0-beta.4"
  },
  "devDependencies": {
    "chance": "^1.0.16",
    "faker": "^4.1.0",
    "sinon": "^6.1.5"
  }
}
