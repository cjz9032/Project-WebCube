{
  "private": true,
  "scripts": {
    "precommit": "webcube-precommit",
    "precommit:custom": "npm run test",
    "update": "npm run clear:deps && yarn install && echo 5.0 > .project-version",
    "upgrade": "npm run clear:lock && npm run update",
    "clear:lock": "rm yarn.lock package-lock.json 2>/dev/null || true",
    "clear:deps": "rm -rf ./node_modules 2>/dev/null || true && rm .git/hooks/pre-commit 2>/dev/null || true",
    "lint": "webcube-lint",
    "lint:error": "webcube-lint-error",
    "test": "webcube-test",
    "build": "webcube-build",
    "build:html": "webcube-build-html",
    "new": "webcube-new",
    "dev": "webcube-dev",
    "dev:dashboard": "webcube-dev-dashboard",
    "staticserver": "webcube-staticserver",
    "start:staticserver": "webcube-start-staticserver",
    "stop:staticserver": "webcube-stop-staticserver",
    "deploy:staticserver": "webcube-deploy-staticserver",
    "deploy:staticserver:push": "webcube-deploy-staticserver-push",
    "deploy:staticweb": "webcube-deploy-staticweb",
    "upload:staticweb:html": "webcube-upload-staticweb-html",
    "upload:staticweb:assets": "webcube-upload-staticweb-assets",
    "webcube:update": "webcube-update",
    "webcube:setup": "webcube-setup"
  },
  "engines": {
    "node": ">=6"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:error --",
      "git add"
    ]
  },
  "config": {
    "webcube": {
      "staticserver": {
        "container": "[registry]/[hub]/[container]"
      }
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "core-decorators": "^0.20.0",
    "graphql-resolvers": "^0.2.2",
    "hifetch": "^3.0.0",
    "immutable": "^3.8.2",
    "immutability-helper": "^2.6.4",
    "is_js": "^0.9.0",
    "localforage": "^1.5.5",
    "lodash": "^4.17.4",
    "lodash-decorators": "^4.5.0",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-common-kit": "^1.0.0",
    "react-display-name": "^0.2.3",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-loaders": "^3.0.1",
    "react-modal": "^3.3.2",
    "react-motion": "^0.5.2",
    "react-notification-system": "^0.2.17",
    "react-router-dom": "^4.2.2",
    "react-spinkit": "^3.0.0",
    "react-with-scripts": "^1.0.0",
    "react-common-kit": "^1.0.0",
    "recompose": "^0.26.0",
    "redux-cube": "^1.0.0-rc.11",
    "redux-cube-with-immutable": "^1.0.5",
    "redux-cube-with-persist": "^1.0.0",
    "redux-cube-with-router": "^1.0.0",
    "redux-form": "^7.2.3",
    "redux-source": "^0.2.0",
    "redux-source-immutable": "^0.2.4",
    "redux-source-connect": "^0.2.0",
    "redux-source-connect-immutable": "^0.2.3",
    "redux-source-with-block-ui": "^1.0.0",
    "redux-source-with-notify": "^1.0.0",
    "rxjs": "^5.5.6",
    "styled-components": "^3.1.6",
    "styled-normalize": "^4.0.0",
    "styled-system": "^2.1.1",
    "validator": "^9.4.1",
    "webcube": "^1.0.0-alpha.37"
  },
  "devDependencies": {
    "chance": "^1.0.13",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-redux": "^0.1.7",
    "faker": "^4.1.0",
    "jest-styled-components": "^5.0.0",
    "react-test-renderer": "^16.2.0",
    "redux-test-utils": "^0.2.2",
    "redux-testkit": "^1.0.6",
    "sinon": "^4.4.2"
  }
}
